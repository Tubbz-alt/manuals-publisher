#!/usr/bin/env ruby

require File.expand_path("../../config/environment", __FILE__)

# TODO Only CMA cases? Should this do more? Do we even need this file?
repository = SpecialistPublisherWiring.get(:cma_case_repository)

repository.all.each do |doc|
  doc.attachments.each do |a|
    a.update_attribute(:file_url, a.file.file_url) if a.file
  end
end
